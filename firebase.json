{
  "hosting": {
    "public": "public",
    "rewrites":[{
      "source": "/home",
      "function": "app"
    },
    {
      "source": "/spotify/setup",
      "function": "spotify_app"
    },
    {
      "source": "/spotify/code",
      "function": "spotify_app"
    },
    {
      "source": "/spotify/latest",
      "function": "spotify_app"
    },
    {
      "source": "/instagram/setup",
      "function": "instagram_app"
    },
    {
      "source": "/instagram/code",
      "function": "instagram_app"
    },
    {
      "source": "/instagram/latest",
      "function": "instagram_app"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
